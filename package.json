{
  "name": "reactium",
  "version": "2.3.9",
  "description": "A framework for creating React + Redux apps using the domain driven design (DDD) paradigm.",
  "main": "index.js",
  "scripts": {
    "start": "node build/.core/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "manifest": "node ./.core/createManifest.js",
    "build": "npm install --production-only",
    "build:gulp": "cross-env NODE_ENV=production gulp",
    "build:babel-core": "cross-env NODE_ENV=production babel .core --out-dir build/.core",
    "build:babel-src": "cross-env NODE_ENV=production babel src --out-dir build/src",
    "static": "npm-run-all build:* && gulp static",
    "local": "gulp local",
    "local-ssr": "gulp local:ssr",
    "clean": "gulp clean",
    "docker": "npm-run-all -s clean docker:*",
    "docker:build": "cross-env docker image build -t $npm_package_name:$npm_package_version -t $npm_package_name:latest .",
    "docker:push": "cross-env docker push $npm_package_name:$npm_package_version && cross-env docker push $npm_package_name:$npm_package_version:latest",
    "deploy": "cf push",
    "precommit": "lint-staged",
    "postinstall": "npm-run-all -s build:*"
  },
  "keywords": [
    "react",
    "redux",
    "framework",
    "javascript",
    "ddd"
  ],
  "author": "Cam Tullos <cam@tullos.ninja> (http://cam.tullos.ninja) and John Dillick <john@dillick.us>",
  "license": "MIT",
  "engines": {
    "node": "9.2.1",
    "npm": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Atomic-Reactor/Reactium.git"
  },
  "browser": {
    "parse/node": false
  },
  "dependencies": {
    "ajv": "^6.5.2",
    "axios": "^0.18.0",
    "beautify": "0.0.8",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "copy-to-clipboard": "^3.0.8",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "directory-tree": "^2.0.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.1.0",
    "globby": "^8.0.1",
    "gsap": "^2.0.1",
    "html-to-jsx": "0.0.4",
    "htmltojsx": "^0.3.0",
    "http-auth": "^3.2.3",
    "js-beautify": "^1.7.5",
    "npm-run-all": "^4.1.2",
    "marked": "^0.4.0",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "object-path": "^0.11.4",
    "parse": "^1.11.0",
    "querystring-browser": "^1.0.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^8.0.1",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-local-persist": "0.0.5",
    "redux-super-thunk": "0.0.6",
    "semver": "^5.5.0",
    "serialize-javascript": "^1.4.0",
    "slug": "^0.9.1",
    "underscore": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.24.5",
    "chalk": "^2.3.0",
    "del": "^3.0.0",
    "fs-extra": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-if": "^2.0.2",
    "gulp-install": "^1.1.0",
    "gulp-less": "^4.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.3.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "handlebars": "^4.0.11",
    "husky": "^0.14.3",
    "ignore-loader": "^0.1.2",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.7",
    "readdir-recursive": "0.0.4",
    "run-sequence": "^2.2.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.16.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "yargs": "^12.0.1"
  },
  "nodemonConfig": {
    "watch": [
      "src/manifest.js"
    ],
    "ignore": [
      "src/app/*"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "stage-2",
      [
        "env",
        {
          "useBuiltIns": true,
          "debug": false,
          "targets": {
            "browsers": [
              ">= 1%"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "cwd": "packagejson",
          "alias": {
            "manifest": "./src/manifest",
            "appdir": "./src/app",
            "components": "./src/app/components",
            "reactium-core": "./.core",
            "dependencies": "./.core/dependencies",
            "toolkit": "./src/app/toolkit"
          }
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
